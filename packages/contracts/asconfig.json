{
  "entries": [
    "assembly/contracts/SimpleStreamManager.ts",
    "assembly/contracts/StreamManagerClean.ts",
    "assembly/contracts/StreamManagerV2.ts"
  ],
  "options": {
    "bindings": "esm",
    "exportRuntime": true,
    "runtime": "stub",
    "optimizeLevel": 3,
    "shrinkLevel": 1,
    "noAssert": false,
    "debug": false
  },
  "targets": {
    "release": {
      "outDir": "build",
      "optimize": true,
      "noAssert": true,
      "bindings": "esm"
    },
    "debug": {
      "outDir": "build/debug",
      "debug": true,
      "optimize": false,
      "sourceMap": true
    }
  }
}
